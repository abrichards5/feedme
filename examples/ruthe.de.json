{
  "items": [
    {
      "search": "#cartoon",
      "do": [
        {
          "find": "a",
          "do": [
            {
              "attr": "href",
              "do": [
                {
                  "regex": "id=(\\d+)",
                  "data": [
                    {
                      "name": "id",
                      "type": "int"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "find": "img",
          "do": [
            {
              "attr": "src",
              "do": [
                {
                  "copy": true,
                  "name": "image",
                  "type": "string"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "transform": {
    "title": "Strip {{.id}}",
    "uri": "/index.php?pic={{.id}}",
    "description": "<img src=\"http://www.ruthe.de/{{.image}}\"/> Strip {{.id}}"
  }
}
